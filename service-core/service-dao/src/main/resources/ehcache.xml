    </cache>                

    <cache name="activity"
           maxElementsInMemory="10000" 
           eternal="false"
           timeToIdleSeconds="240" 
           timeToLiveSeconds="240" 
           diskExpiryThreadIntervalSeconds="240"
           memoryStoreEvictionPolicy="LRU">
        <persistence strategy="localTempSwap" />
